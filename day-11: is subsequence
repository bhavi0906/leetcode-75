class Solution {
public:
    bool isSubsequence(string s, string t) {
      int a=0;
      int flag=0;
      while(a<s.size() && flag < t.length())
      {
       
        if(s[a]==t[flag])
        {
            a++;
        }
        flag++;
       }
       

       if(a==s.size())
       return true;
       else
       return false;


        

        
        
    }
};
